{
    "$schema": "./../.schema/engine.json",
    "name": "colorguard",
    "description": "CSS Colorguard helps you maintain the color set that you want, and warns you when colors you've added are too similar to ones that already exist",
    "url": "https://github.com/SlexAxton/css-colorguard",
    "languages": [
        "css"
    ],
    "extensions": [
        "*.css"
    ],
    "license": "MIT",
    "areas": [
        "formatting"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "colorguard",
            "version": "1.2.0"
        }
    ],
    "postfix": " | node file://{schema}/colorguard/reporter.js",
    "acceptMask": false,
    "version": "1.2.0",
    "defaults": {
        "--file": "*"
    }
}
