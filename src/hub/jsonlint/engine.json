{
    "$schema": "./../.schema/engine.json",
    "name": "jsonlint",
    "description": "A pure JavaScript version of the service provided at jsonlint.com", 
    "version": "1.6.2",
    "url": "https://github.com/zaach/jsonlint",
    "languages": [
        "json"
    ],
    "extensions": [
        "*.json"
    ],
    "license": "MIT",
    "areas": [
        "syntax",
        "formatting"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "jsonlint", 
            "version": "1.6.2"
        }
    ],
    "postfix": " 2>&1 | node file://{schema}/jsonlint/reporter.js",
    "acceptMask": false,
    "defaults": {
        "": "*",
        "--quiet": null,
        "--compact": null
    }
}
