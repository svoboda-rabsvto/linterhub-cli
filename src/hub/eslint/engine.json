{
    "$schema": "./../.schema/engine.json",
    "name": "eslint",
    "description": "A fully pluggable tool for identifying and reporting on patterns in JavaScript",
    "url": "http://eslint.org",
    "version": "v4.0.0",
    "languages": [
        "javascript",
        "jsx"
    ],
    "extensions": [
        "*.js"
    ],
    "configs": [
        ".eslintrc",
        ".eslintrc.json",
        ".eslintrc.yaml",
        ".eslintrc.yml",
        ".eslintrc.js"
    ],
    "license": "MIT",
    "areas": [
        "syntax"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "eslint", 
            "version": "4.0.0"
        },
        {
            "manager": "npm",
            "package": "eslint-config-google"
        }
    ],
    "defaults": {
        "--format": "file://{schema}/eslint/reporter.js",
        "--config": "file://{schema}/eslint/.eslintrc.js",
        "": "**/**/*.js"
    },
    "stdin":
    {
        "--stdin": null,
        "--stdin-filename": "file://{stdin}"
    },
    "optionsDelimiter": "="
}
