{
    "$schema": "./../.schema/engine.json",
    "name": "jshint",
    "description": "JSHint is a tool that helps to detect errors and potential problems in your JavaScript code",
    "version": "jshint v2.9.5",
    "url": "http://jshint.com",
    "languages": [
        "javascript"
    ],
    "extensions": [
        "*.js"
    ],
    "configs": [
        ".jshintrc"
    ],
    "license": "MIT",
    "areas": [
        "syntax",
        "formatting",
        "complexity",
        "unused code"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "jshint",
            "version": "2.9.5"
        }
    ],
    "defaults": {
        "": "./",
        "--reporter": "file://{schema}/jshint/reporter.js"
    },
    "stdin": {
        "--filename": "file://{stdin}",
        "-": null
    },
    "optionsDelimiter": "=",
    "successCode": 2
}
