{
    "$schema": "./../.schema/engine.json",
    "name": "csslint",
    "description": "Automated linting of Cascading Stylesheets",
    "url": "http://csslint.net",
    "languages": [
        "css"
    ],
    "extensions": [
        "*.css"
    ],
    "configs": [
        ".csslintrc"
    ],
    "license": "MIT",
    "areas": [
        "syntax",
        "code simplification"
    ],
    "requirements": [
        {
            "manager": "system",
            "package": "npm"
        },
        {
            "manager": "npm",
            "package": "csslint",
            "version": "1.0.5"
        }
    ],
    "version": "v1.0.5",
    "postfix": " | node file://{schema}/csslint/reporter.js",
    "acceptMask": false,
    "optionsDelimiter": "=",
    "defaults": {
        "": "*",
        "--format": "compact"
    }
}
