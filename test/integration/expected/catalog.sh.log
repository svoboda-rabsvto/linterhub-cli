  {
    "name": "coffeelint",
    "description": "CoffeeLint is a style checker that helps keep CoffeeScript code clean and consistent",
    "version": {
      "local": "1.16.0"
    },
    "url": "http://www.coffeelint.org",
    "languages": [
      "coffeescript"
    ],
    "license": "MIT",
    "areas": [
      "syntax",
      "formatting",
      "smell",
      "complexity",
      "duplication"
    ],
    "extensions": [
      "*.coffee"
    ],
    "requirements": [
      {
        "manager": "system",
        "package": "npm"
      },
      {
        "manager": "npm",
        "package": "coffeelint"
      }
    ],
    "active": false
  },
